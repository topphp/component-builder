{
    "name": "topphp/component-builder",
    "type": "library",
    "description": ":package_description",
    "keywords": [
        "vendor",
        "package_name"
    ],
    "homepage": "https://github.com/topphp/component-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "author_name",
            "email": "author_email",
            "homepage": "author_website",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            ":vendor\\:package_name\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            ":vendor\\:package_name\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests",
        "post-root-package-install": [
            "@php prefill.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
